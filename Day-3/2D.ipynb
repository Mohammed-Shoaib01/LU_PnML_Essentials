        "import numpy as np",
        "x=int(input(\"X = \"))\n",
        "y=int(input(\"Y = \"))\n",
        "z=np.zeros((x,y))\n",
        "for r in range(x):\n",
        "  for c in range(y):\n",
        "    z[r,c]=r*c\n",
        "print(z)\n"
